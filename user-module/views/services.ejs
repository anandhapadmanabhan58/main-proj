<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>JARVISnet</title>
  </head>
  <link rel="stylesheet" href="css/maicons.css" />

  <link rel="stylesheet" href="css/bootstrap.css" />

  <link rel="stylesheet" href="vendor/animate/animate.css" />

  <link rel="stylesheet" href="css/theme.css" />
  <body>
    <!-- Back to top button -->
    <div class="back-to-top"></div>

    <%- include('./partials/header')-%>

    <div class="container">
      <div class="page-banner home-banner">

        <div class="page-section">
            <div class="container">
              <div class="text-center wow fadeInUp">
                <div class="subhead">SERVICES OFFERED</div>
                <h2 class="title-section">Choose plan the right for you</h2>
                <div class="divider mx-auto"></div>
              </div>
              <div class="row mt-5">
                <div class="col-lg-4 py-3 wow zoomIn">
                  <div class="card-pricing">
                    <div class="header">
                      <div class="pricing-type" style="display:none;">Basic</div>
                      <div class="price">
                        <span class="dollar"></span>
                        <h1>CABLE<span class="suffix"><br>Plans</span></h1>
                      </div>
                      <h5 style="display:none">Per Month</h5>
                    </div>
                    <div class="body">
                      <p>Standard Definition <span class="suffix">Support</span></p>
                      <p>HD <span class="suffix">Support</span></p>
                      <p>Full HD <span class="suffix">Support</span></p>
                      <p>4K<span class="suffix">Support</span></p>
                      <p>24/7 <span class="suffix">Service</span></p>
                    </div>
                    <div class="footer">
                      <a href="cableProviders" class="btn btn-pricing btn-block">Buy Now</a>
                    </div>
                  </div>
                </div>
      
                <div class="col-lg-4 py-3 wow zoomIn">
                  <div class="card-pricing marked">
                    <div class="header">
                      <div class="pricing-type" style="display:none;">Standar</div>
                      <div class="price">
                        <span class="dollar"></span>
                        <h1>NEWS <span class="suffix"><br>Portal</span></h1>
                      </div>
                      <h5 style="display:none">Per Month</h5>
                    </div>
                    <div class="body">
                      <p>General <span class="suffix">News</span></p>
                      <p>Business<span class="suffix">News</span></p>
                      <p>Entertainment <span class="suffix">News</span></p>
                      <p>1 Sports <span class="suffix">News</span></p>
                      <p>24/7<span class="suffix">Service</span></p>
                    </div>
                    <div class="footer">
                      <a href="http://localhost:3000" target="_blank" class="btn btn-pricing btn-block">Subscribe</a>
                    </div>
                  </div>
                </div>
      
                <div class="col-lg-4 py-3 wow zoomIn">
                  <div class="card-pricing">
                    <div class="header">
                      <div class="pricing-type" style="display:none;">Prof</div>
                      <div class="price">
                        <span class="dollar"></span>
                        <h1>BROADBAND<span class="suffix"><br>Plans</span></h1>
                      </div>
                      <h5 style="display:none">Per Month</h5>
                    </div>
                    <div class="body">
                      <p>Latest<span class="suffix"> Modems</span></p>
                      <p>Optic<span class="suffix"> Fibre</span></p>
                      <p>4G <span class="suffix"> Support</span></p>
                      <p>5g <span class="suffix"> Support</span></p>
                      <p>24/7<span class="suffix"> Service</span></p>
                    </div>
                    <div class="footer">
                      <a href="broadbandProviders" class="btn btn-pricing btn-block">Buy Now</a>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        


      </div>
    </div>

    <script src="js/jquery-3.5.1.min.js"></script>

    <script src="js/bootstrap.bundle.min.js"></script>

    <script src="js/google-maps.js"></script>

    <script src="vendor/wow/wow.min.js"></script>

    <script src="js/theme.js"></script>
  </body>
</html>
