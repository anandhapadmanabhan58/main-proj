<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>JARVISnet</title>
    <link rel="stylesheet" href="../css/maicons.css" />

    <link rel="stylesheet" href="../css/bootstrap.css" />

    <link rel="stylesheet" href="../vendor/animate/animate.css" />

    <link rel="stylesheet" href="../css/theme.css" />
  </head>
  <body>
    <!-- Back to top button -->
    <div class="back-to-top"></div>

    <%- include('../../partials/header')-%>

    <div class="page-section">
      <div class="container">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">NO.</th>
              <th scope="col">PLAN NAME</th>
              <th scope="col">PRICE</th>
              <th scope="col" >Data in GB</th>
              <th scope="col">DELETE</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0;i<sp.length;i++) {%>
            <tr>
              <th scope="row"><%=i+1 %></th>
              <td><%=sp[i].BserviceProvider %></td>
              <td><%=sp[i].Bprice %></td>
              <td><%=sp[i].BplanDescription %></td>
              <td><form method="post" action="http://localhost:4050/api/v1/deleteBroaband/<%=sp[i].id%>"><button type="submit" name="sub" class="btn btn-dark">DELETE</button></form></td>
            </tr>
            <%} %>
          </tbody>
        </table>
      </div>
    </div>

    <!--footer-->

    <%-include('../../partials/footer.ejs') -%>

    <script src="../js/jquery-3.5.1.min.js"></script>

    <script src="../js/bootstrap.bundle.min.js"></script>

    <script src="../js/google-maps.js"></script>

    <script src="../vendor/wow/wow.min.js"></script>

    <script src="js/theme.js"></script>
  </body>
</html>
